act gene() {
    _L dose Year = 0;  // Initialize with default value
    Year = stimuli("Enter a year: ");  // Get user input

    // Check leap year conditions
    if ((Year % 4 == 0) && (Year % 100 != 0)) {
        express("Year " + seq(Year) + " is a leap year");
    } elif (Year % 400 == 0) {
        express("Year " + seq(Year) + " is a leap year");
    } else {
        express("Year " + seq(Year) + " is not a leap year");
    }
} 